{
  "name": "@puppet/ivy-tabs",
  "version": "4.0.4",
  "description": "WAI-ARIA accessible tabs for Ember. Originally created by Envy Labs",
  "keywords": [
    "ember",
    "ember-addon",
    "tabs",
    "aria",
    "accessibility"
  ],
  "homepage": "https://github.com/puppetlabs/ivy-tabs#readme",
  "bugs": {
    "url": "https://github.com/puppetlabs/ivy-tabs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puppetlabs/ivy-tabs.git"
  },
  "license": "MIT",
  "author": "Dray Lacy",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "commit": "git-cz",
    "deploy-docs": "node tools/gh-pages-publish.js",
    "format": "prettier --single-quote --write './*.js' './{addon,app,config,tests}/{**/,}*.js'",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@types/prettier": "^2.0.2",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~4.8.0",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-test-loader": "^3.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "^4.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.16.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.88.1"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://ivyapp.github.io/ivy-tabs/"
  }
}
